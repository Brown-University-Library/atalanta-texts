                     <lb n="16"/>falcatas erigit caudas: contra <expan><abbr>serp<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>tes</abbr></expan> pro gallinis dimicat: <milestone unit="section" n="e30-07"/><expan><abbr>N<hi rend="tilde">u</hi></abbr><ex>n</ex><abbr>cius</abbr></expan>
                     <lb n="17"/>est lucis <g type="ampersand">et&gt;</g> <persName>Laton<hi rend="ligature">ae</hi></persName> charus, <expan><abbr><hi rend="stroke">q</hi></abbr><ex>ui</ex><abbr>a</abbr></expan> ei parienti astiterit: <expan><abbr>Pe<hi rend="stroke">p</hi></abbr><ex>er</ex><abbr>it</abbr></expan> ver<hi rend="grave">o</hi> <persName>Latona</persName>
                     <lb n="18"/><expan><abbr>sol<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> <g type="ampersand">et&gt;</g> <expan><abbr>lun<hi rend="tilde">a</hi></abbr><ex>m</ex></expan>, unde matri <g type="ampersand">et&gt;</g> filio appropriatur: <milestone unit="section" n="e30-8"/>At Sol, Luna <g type="ampersand">et&gt;</g> <persName>Latona</persName>
