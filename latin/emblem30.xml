                     <lb n="22"/>dummod<hi rend="grave">o</hi> <expan><abbr>fove<hi rend="tilde">a</hi></abbr><ex>n</ex><abbr>tur</abbr></expan> <expan><abbr>t<hi rend="tilde">e</hi></abbr><ex>m</ex><abbr>perato</abbr></expan> calore, prout gallin<hi rend="ligature">ae</hi> <expan><abbr>incub<hi rend="tilde">a</hi></abbr><ex>n</ex><abbr>tis</abbr></expan> calor
                     <lb n="23"/>est, <expan><abbr><hi rend="stroke">q</hi></abbr><ex>ui</ex></expan> continu<hi rend="grave">e</hi> durat: C<hi rend="grave">u</hi>m <expan><ex>e</ex><abbr>n<am>.</am></abbr><ex>im</ex></expan> in c<hi rend="ligature">ae</hi>teris <expan><abbr>volucrib<am>.</am></abbr><ex>us</ex></expan> <g type="ampersand">et&gt;</g> masculus ovis in
                     <lb n="24" break="no"/>sideat, solus gallus ab hoc officio sive onere sese immunem ostendit;
                     <lb n="25"/><milestone unit="section" n="e30-10"/>Unde gallin<hi rend="ligature">ae</hi> omnis cura <g type="ampersand">et&gt;</g> labor ova <expan><abbr>exclud<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>di</abbr></expan>, <g type="ampersand">et&gt;</g> pullos educandi
