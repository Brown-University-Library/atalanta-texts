                  <div type="discourse-p1"><ab>
                     <lb n="1"/><milestone unit="section" n="e30-01"/><persName ref="https://viaf.org/viaf/89770781">Avicenna</persName> in <expan><abbr>lib<am>.</am></abbr><ex>ro</ex></expan> <title>de anima</title> aliquotiens monet, ova <expan><abbr>n<hi rend="tilde">o</hi></abbr><ex>n</ex></expan> esse ad <expan><abbr>art<hi rend="tilde">e</hi></abbr><ex>m</ex></expan>
                     <lb n="2"/>sumenda, nisi <expan><abbr>ear<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> <expan><abbr>gallinar<hi rend="tilde">u</hi></abbr><ex>m</ex></expan>, <expan><abbr>quib<am>.</am></abbr><ex>us</ex></expan> gallus adfuerit: hoc est, <expan><abbr>absq<am>;</am></abbr><ex>ue</ex></expan>
                     <lb n="3"/>virtute masculina f<hi rend="ligature">oe</hi>mineum subjectum nihil valere, <g type="ampersand">et&gt;</g> vice versa,
                     <lb n="4"/>nec <expan><abbr>gall<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> <expan><abbr>absq<am>;</am></abbr><ex>ue</ex></expan> gallina <expan><abbr>ultil<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> fore: <milestone unit="section" n="e30-02"/>Hi <expan><ex>e</ex><abbr>n<am>.</am></abbr><ex>im</ex></expan> gemini sexus in corte <expan><abbr>Philos<am>.</am></abbr><ex>ophico</ex></expan>
                     <lb n="5"/><expan><abbr>conj<hi rend="tilde">u</hi></abbr><ex>n</ex><abbr>gendi</abbr></expan> sunt, ut multplicatio fiat: <expan><abbr>Philos<am>.</am></abbr><ex>ophi</ex></expan> ver<hi rend="grave">o</hi> utuntur similitu
                     <lb n="6" break="no"/>dine <hi rend="grave">a</hi> gallo desumpt<hi rend="circumflex">a</hi>, quia hic inprimis sulphuris <expan><abbr>pot<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>ti<hi rend="ligature">ae</hi></abbr></expan> <expan><abbr>correp<hi rend="tilde">o</hi></abbr><ex>n</ex>
                     <lb n="7" break="no"/><abbr>det</abbr></expan> magis, qu<hi rend="grave">a</hi>m alius masculus ex avibus c<hi rend="grave">u</hi>m unus multis gallinis
                     <lb n="8"/>pr<hi rend="ligature">ae</hi>esse possit, nec facil<hi rend="grave">e</hi> <expan><abbr>rival<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> in <expan><abbr>eod<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> fimeto patiatur, sese <expan><abbr>par<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> <g type="ampersand">et&gt;</g>
                     <lb n="9"/><expan><abbr>sufficient<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> singuls existimans. <milestone unit="section" n="e30-03"/><persName>Martis</persName> avis est ex Gallo puero, <expan><abbr><hi rend="stroke">q</hi></abbr><ex>ui</ex></expan> <expan><abbr>Sol<hi rend="tilde">e</hi></abbr><ex>m</ex></expan>
                     <lb n="10"/>observare deberet, ne <expan><abbr>adulteri<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> <persName>Martis</persName> cum <persName>Venere</persName> specularetur,
                     <lb n="11"/>transformata, ut <expan><abbr>fabul<hi rend="tilde">a</hi></abbr><ex>n</ex><abbr>tur</abbr></expan> Poët<hi rend="tilde">ae</hi>, <g type="ampersand">et&gt;</g> Martialis vald<hi rend="grave">e</hi> in pugn<hi rend="circumflex">a</hi>, qua
                     <lb n="12"/>decertat cum adversario <expan><abbr>usq<am>;</am></abbr><ex>ue</ex></expan> ad <expan><abbr>nec<hi rend="tilde">e</hi></abbr><ex>m</ex></expan>: <milestone unit="section" n="e30-4"/>Solem in opere <expan><abbr>Philos<am>.</am></abbr><ex>ophico</ex></expan> repr<hi rend="ligature">ae</hi>
                     <lb n="13" break="no"/>sentat, ut gallina <expan><abbr>lun<hi rend="tilde">a</hi></abbr><ex>m</ex></expan>: <expan><abbr>Ead<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> <expan><abbr>itaq<am>;</am></abbr><ex>ue</ex></expan> necessitas <expan><abbr>conjung<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>di</abbr></expan> <expan><abbr>Sol<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> cum lu
                     <lb n="14" break="no"/>na, qu<hi rend="grave">a</hi>m <expan><abbr>gall<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> cum gallina: <milestone unit="section" n="e30-05"/>Est <expan><abbr>quoq<am>;</am></abbr><ex>ue</ex></expan> gallus Soli sacer, cui venienti
