                     <lb n="6" break="no"/>nus</persName> in vinis, ignis in igne furit</quote>: <milestone unit="section" n="e10-04"/> Ver<hi rend="grave">u</hi>m <expan><abbr>resp<hi rend="tilde">o</hi></abbr><ex>n</ex><abbr>dendum</abbr></expan>, <expan><abbr><hi rend="stroke">q</hi></abbr><ex>uod</ex></expan> ignis <g type="ampersand">et</g> ignis,
