                     <lb n="3" break="no"/>mne<expan><abbr>n<am>.</am></abbr><ex>m</ex></expan> simile suo simili additum, magis fit tale: <milestone unit="section" n="e10-02"/> Hinc medici <expan><abbr>c<hi rend="tilde">o</hi></abbr><ex>n</ex><abbr>traria</abbr></expan>
                     <lb n="4"/><expan><abbr>c<hi rend="tilde">o</hi></abbr><ex>n</ex><abbr>trariis</abbr></expan> mederi, <g type="ampersand">et</g> <expan><abbr>h</abbr><ex>a</ex><abbr><hi rend="hook">e</hi>c</abbr></expan> ab illis toli <expan><abbr>asser<hi rend="tilde">u</hi></abbr><ex>n</ex><abbr>t</abbr></expan>, ita <expan><abbr>ign<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> ab aqua extingui,
