                     <lb n="12"/>frigore hyberno torpidos, <g type="ampersand">et</g> fer<hi rend="grave">e</hi> emortuos aqu<hi rend="circumflex">a</hi> frigid<hi rend="circumflex">a</hi>, si in eam
                     <lb n="13"/><expan><abbr>merg<hi rend="tilde">a</hi></abbr><ex>n</ex><abbr>tur</abbr></expan>, <expan><abbr>n<hi rend="tilde">o</hi></abbr><ex>n</ex></expan> calore externo immediat<hi rend="grave">e</hi> adhibito, restaurari: <milestone unit="section" n="e10-06"/> Ut ve
                     <lb n="14" break="no"/>r<hi rend="grave">o</hi> <expan><abbr>lum<hi rend="tilde">e</hi></abbr><ex>n</ex></expan> majus obscurat minus, sic <g type="ampersand">et</g> major calor minorem, <g type="ampersand">et</g> frigus
                     <lb n="15"/>majus, minus: Nihilomin<hi rend="grave">u</hi>s oportet <expan><abbr>calor<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> <g type="ampersand">et</g> frigus <expan><abbr>extern<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> minus
                     <lb n="16"/>esse, qu<hi rend="grave">a</hi>m ante impressum membris aut <expan><abbr>artub<am>.</am></abbr><ex>us</ex></expan> ali<hi rend="grave">a</hi>s <expan><abbr>ead<hi rend="grave">e</hi></abbr><ex>m</ex></expan> impressio
                     <lb n="17"/>fieret, qu<hi rend="ligature">ae</hi> ante, <g type="ampersand">et</g> simile <hi rend="grave">a</hi> simili mult<hi rend="grave">o</hi> magis augeretur, <expan><abbr>qu<hi rend="tilde">a</hi></abbr><ex>m</ex></expan> evoca
                     <lb n="18" break="no"/>retur: <milestone unit="section" n="e10-07"/> Evocatio <expan><ex>e</ex><abbr>n<am>.</am></abbr><ex>im</ex></expan> firgoris ab aqua frigida, <g type="ampersand">et</g> caloris ignei <hi rend="grave">a</hi> calore,
                     <lb n="19"/>natur<hi rend="ligature">ae</hi> <expan><abbr>conveni<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>s</abbr></expan> est, quia omnis <expan><abbr>rep<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>tina</abbr></expan> in <expan><abbr>contrari<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> mutatio ei
                     <lb n="20"/>periculosa, <g type="ampersand">et</g> min<hi rend="grave">u</hi>s accepta est, <expan><abbr>qu</abbr><ex>a</ex><abbr><hi rend="hook">e</hi></abbr></expan> ver<hi rend="grave">o</hi> fit sensim, <g type="ampersand">et</g> quasi per gra
                     <lb n="21" break="no"/>dus tolerabilis: <milestone unit="section" n="e10-08"/> Ita dicimus, <expan><abbr>ali<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> <expan><abbr>ign<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> esse <expan><abbr>intern<hi rend="tilde">u</hi></abbr><ex>m</ex></expan>, <expan><abbr>in<hi rend="acute">q</hi><am>;</am></abbr><ex>que</ex></expan> subjecto <expan><abbr>Phi
                     <lb n="22" break="no"/>los<am>.</am></abbr><ex>ophico</ex></expan> jam ante <expan><abbr>infix<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> seu <expan><abbr>essential<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> existere, <expan><abbr>ali<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> <expan><abbr>extern<hi rend="tilde">u</hi></abbr><ex>m</ex></expan>, <expan><abbr><hi rend="stroke">q</hi></abbr><ex>uod</ex></expan> quoque de
                     <lb n="23"/>Mercurio intelligendum. <milestone unit="section" n="e10-09"/> Internus ille ignis est <hi rend="ligature">ae</hi>quivoc<hi rend="grave">e</hi> propter i
                     <lb n="24" break="no"/>gneas qualitates, vires <g type="ampersand">et</g> operationes, externus univoc<hi rend="grave">e</hi>. <expan><abbr>D<hi rend="tilde">a</hi></abbr><ex>n</ex><abbr>dus</abbr></expan> igi
                     <lb n="25" break="no"/>tur ignis externus interno <g type="ampersand">et</g> ejusmodi Mercurius Mercurio, ut artis
                     <lb n="26"/><expan><abbr>int<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>tio</abbr></expan> fiat: Ad omne <expan><abbr>dur<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> aut <expan><abbr>crud<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> coctione <expan><abbr>emolli<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>dum</abbr></expan> vel <expan><abbr>matu
                     <lb n="27" break="no"/>r<hi rend="tilde">a</hi>nd<hi rend="tilde">u</hi></abbr><ex>m</ex></expan>, igne <g type="ampersand">et</g> aqua utimur: <milestone unit="section" n="e10-10"/> Aqua <expan><ex>e</ex><abbr>n<am>.</am></abbr><ex>im</ex></expan> <expan><abbr>duriti<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> solvit <g type="ampersand">et</g> in partes <expan><abbr>c<hi rend="tilde">o</hi></abbr><ex>m</ex><abbr>pa</abbr>
                     <lb n="28" break="no"/>ctas</expan> penetratm calore vires ei addente <g type="ampersand">et</g> <expan><abbr>mot<hi rend="tilde">u</hi></abbr><ex>m</ex></expan>, ut apparet. <expan><abbr>E<am>.</am></abbr><ex>xempli</ex></expan> <expan><abbr>g<am>.</am></abbr><ex>ratia</ex></expan> in co
                     <lb n="29" break="no"/>ctione <expan><abbr>pisor<hi rend="tilde">u</hi></abbr><ex>m</ex></expan>, qu<hi rend="ligature">ae</hi> per se sunt integra <g type="ampersand">et</g> dura, ver<hi rend="grave">u</hi>m ab aqua <expan><abbr>intu
                     <lb n="30" break="no"/>umesc<hi rend="tilde">u</hi></abbr><ex>n</ex><abbr>t</abbr></expan>, <expan><abbr>frang<hi rend="tilde">u</hi></abbr><ex>n</ex><abbr>tur</abbr></expan> <g type="ampersand">et</g> in <expan><abbr>pult<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> <expan><abbr>redig<hi rend="tilde">u</hi></abbr><ex>n</ex><abbr>tur</abbr></expan>, calore ignis per <expan><abbr>ebullition<hi rend="tilde">e</hi></abbr><ex>m</ex></expan>
                     <lb n="31"/><expan><abbr>aqu<hi rend="tilde">a</hi></abbr><ex>m</ex></expan> <expan><abbr>rarefaci<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>te</abbr></expan>, <g type="ampersand">et</g> tenu<hi rend="acute">i</hi>oris seu fer<hi rend="grave">e</hi> aëri<hi rend="ligature">ae</hi> <expan><abbr>subst<hi rend="tilde">a</hi></abbr><ex>n</ex><abbr>ti<hi rend="ligature">ae</hi></abbr></expan> <expan><abbr>redd<hi rend="tilde">e</hi></abbr><ex>n</ex><abbr>te</abbr></expan>: Sic i
                     <lb n="32" break="no"/>gnis calor crudas partes <expan><abbr>fructu<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> aut <expan><abbr>carni<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> resolvit in <expan><abbr>aqu<hi rend="tilde">a</hi></abbr><ex>m</ex></expan>, <expan><abbr>c<hi rend="tilde">u</hi></abbr><ex>m</ex><abbr><hi rend="acute">q</hi><am>.</am></abbr><ex>ue</ex></expan> ea
                     <lb n="33"/>in <expan><abbr>aër<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> evanescere facit: <milestone unit="section" n="e10-11"/> Eodem modo ignis <g type="ampersand">et</g> Mercurius hic sunt i
                     <lb n="34" break="no"/>gnis <g type="ampersand">et</g> aqua, ac iidem, ignis <g type="ampersand">et</g> Mercurius sunt partes matur<hi rend="ligature">ae</hi> <g type="ampersand">et</g> cru
