                        <lb n="15" break="no"/>m<hi rend="tilde">a</hi></abbr><ex>m</ex></expan> <g type="ampersand">et</g> <expan><abbr>spirit<hi rend="tilde">u</hi></abbr><ex>m</ex></expan>, super quo dicit: Draco <expan><abbr>n<hi rend="tilde">o</hi></abbr><ex>n</ex></expan> moritur, nisi <expan><abbr>c<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> fratre suo <g type="ampersand">et</g>
