                     <lb n="9"/>qu<hi rend="ligature">ae</hi> fiebant in <expan><abbr>honor<hi rend="tilde">e</hi></abbr><ex>m</ex></expan> <persName type="divinity">Osiridis</persName>, <hi rend="grave">a</hi> <expan><abbr>sacerdotib<am>.</am></abbr><ex>is</ex></expan> mitratis, rasis capitibus
