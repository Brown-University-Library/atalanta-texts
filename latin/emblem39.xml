                     <lb n="2"/>precii: <expan><abbr>Thesaur<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> <expan><ex>e</ex><abbr>n<am>.</am></abbr><ex>im</ex></expan> <expan><abbr>maxim<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> <g type="ampersand">et</g> munus Dei <expan><abbr>excellentissim<hi rend="tilde">u</hi></abbr><ex>m</ex></expan> qu<hi rend="ligature">ae</hi>
