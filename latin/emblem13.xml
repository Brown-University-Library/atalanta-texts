                     <lb n="30" break="no"/>quis ab aquis liberari poterit? sed <expan><abbr>resp<hi rend="tilde">o</hi></abbr><ex>n</ex><abbr>dend<hi rend="tilde">u</hi></abbr><ex>m</ex></expan>, esse <expan><abbr>quasd<hi rend="tilde">a</hi></abbr><ex>m</ex></expan> aquas ca
                     <lb n="31" break="no"/>lidas <g type="ampersand">et</g> siccas in qualitate, quales sunt mult<hi rend="ligature">ae</hi> <expan><abbr>therm</abbr><ex>a</ex><abbr><hi rend="hook">e</hi></abbr></expan>, in <expan><abbr>quib<am>.</am></abbr><ex>us</ex></expan> <expan><abbr>Philos<am>.</am></abbr><ex>ice</ex></expan>
